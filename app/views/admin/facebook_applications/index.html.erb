<h1>Listing admin_facebook_applications</h1>

<table>
  <thead>
    <tr>
      <th>App name</th>
      <th>App id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_facebook_applications.each do |admin_facebook_application| %>
      <tr>
        <td><%= admin_facebook_application.name %></td>
        <td><%= admin_facebook_application.app_id %></td>
        <td><%= link_to 'Show', admin_facebook_application %></td>
        <td><%= link_to 'Edit', edit_admin_facebook_application_path(admin_facebook_application) %></td>
        <td><%= link_to 'Destroy', admin_facebook_application, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Facebook application', new_admin_facebook_application_path %>
